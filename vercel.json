{
    "headers": [
        {
            "source": "/fonts/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "rewrites": [
        { "source": "/guides/:path*", "destination": "https://guides.frii.site/:path*" }
    ]
}